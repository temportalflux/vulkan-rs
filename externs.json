[
	{
		"name": "Simple DirectMedia Layer 2",
		"alias": "sdl2",
		"url": "https://www.libsdl.org/",
		"downloads": [
			{
				"url": "https://www.libsdl.org/release/SDL2-devel-2.0.14-VC.zip",
				"contents": [
					{
						"glob": "SDL2-.*/lib/x64/.*\\.dll",
						"destination": "x64"
					},
					{
						"glob": "SDL2-.*/lib/x64/SDL2.lib",
						"destination": "x64"
					},
					{
						"glob": "SDL2-.*/lib/x86/.*\\.dll",
						"destination": "x32"
					},
					{
						"glob": "SDL2-.*/lib/x86/SDL2.lib",
						"destination": "x32"
					}
				]
			}
		]
	}
]